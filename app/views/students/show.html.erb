<h1>Student Details<h1>

<%= notice %>

<p>Name: <%= @student.full_name %></p>
<p>Age: <%= @student.age %></p>
<p>Email: <%= @student.email %></p>
<p>Email: <%= formatted_date(@student.date_of_birth) %></p>
<p>Email: <%= @student.local_address %></p>
<p>Email: <%= @student.permanent_address %></p>
<p>Email: <%= @student.contact %></p>


<%= link_to 'Back', students_path %> | <%= link_to 'Edit', edit_student_path(@student) %>